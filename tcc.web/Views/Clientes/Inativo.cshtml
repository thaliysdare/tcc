@using tcc.web.Utils;
@model ClienteViewModel
@{
    ViewData["Title"] = "Clientes";
}

<form class="row g-3">

    <div class="col-2">
        <label asp-for="Nome" class="form-label"></label>
        <input asp-for="Nome" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>
    <div class="col-3">
        <label asp-for="Sobrenome" class="form-label"></label>
        <input asp-for="Sobrenome" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="Sobrenome" class="text-danger"></span>
    </div>
    <div class="col-3">
        <label asp-for="CPFOuCNPJ" class="form-label"></label>
        <input asp-for="CPFOuCNPJ" type="number" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="CPFOuCNPJ" class="text-danger"></span>
    </div>
    <div class="col-2">
        <label asp-for="Telefone1" class="form-label"></label>
        <input asp-for="Telefone1" type="tel" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="Telefone1" class="text-danger"></span>
    </div>
    <div class="col-2">
        <label asp-for="Telefone2" class="form-label"></label>
        <input asp-for="Telefone2" type="tel" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="Telefone2" class="text-danger"></span>
    </div>
    <div class="col-3">
        <label asp-for="EnderecoViewModel.Rua" class="form-label"></label>
        <input asp-for="EnderecoViewModel.Rua" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="EnderecoViewModel.Rua" class="text-danger"></span>
    </div>
    <div class="col-1">
        <label asp-for="EnderecoViewModel.Numero" class="form-label"></label>
        <input asp-for="EnderecoViewModel.Numero" type="number" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="EnderecoViewModel.Numero" class="text-danger"></span>
    </div>
    <div class="col-2">
        <label asp-for="EnderecoViewModel.CEP" class="form-label"></label>
        <input asp-for="EnderecoViewModel.CEP" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="EnderecoViewModel.CEP" class="text-danger"></span>
    </div>
    <div class="col-6">
        <label asp-for="EnderecoViewModel.Complemento" class="form-label"></label>
        <input asp-for="EnderecoViewModel.Complemento" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="EnderecoViewModel.Complemento" class="text-danger"></span>
    </div>
    <div class="col-3">
        <label asp-for="EnderecoViewModel.Bairro" class="form-label"></label>
        <input asp-for="EnderecoViewModel.Bairro" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="EnderecoViewModel.Bairro" class="text-danger"></span>
    </div>
    <div class="col-3">
        <label asp-for="EnderecoViewModel.Cidade" class="form-label"></label>
        <input asp-for="EnderecoViewModel.Cidade" type="text" class="form-control form-control-sm" readonly/>
        <span asp-validation-for="EnderecoViewModel.Cidade" class="text-danger"></span>
    </div>
    <div class="col-3">
        <label asp-for="EnderecoViewModel.Estado" class="form-label"></label>
        <select asp-for="EnderecoViewModel.Estado" class="form-select form-select-sm" disabled>
        </select>
    </div>
</form>

<partial name="_VeiculosGridInativo.cshtml" />



